<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{mm:ss} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="error">
        <appender-ref ref="STDOUT" />
    </root>

    <logger name="it.infocert.eigor.api.xml.CachedResourceResolver" level="all" />
    <logger name="it.infocert" level="DEBUG" />
    <logger name="it.infocert.eigor.api.conversion" level="error" />

</configuration>